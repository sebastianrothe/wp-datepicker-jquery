{
  "name": "datepicker-jqueryui",
  "version": "1.1.0",
  "description": "A Datepicker in JQuery UI with disabled dates and formatting",
  "main": "src/index.js",
  "scripts": {
    "start": "serve public/bundle.custom.html public/data/data.txt",
    "test": "jest --watch",
    "clean": "rm -rf bundle/** && rm -rf build/** && rm -rf dist/**",
    "copy": "./scripts/copy.sh $npm_package_version bundle/custom",
    "build": "parcel build src/**/*.* --out-dir build --experimental-scope-hoisting",
    "bundle": "parcel build public/bundle.html --out-dir bundle --no-content-hash --experimental-scope-hoisting",
    "bundle:custom": "parcel build public/bundle.custom.html --out-dir bundle/custom --no-content-hash --experimental-scope-hoisting",
    "prepare": "run-s clean bundle:custom copy",
    "postversion": "run-s prepare",
    "plugin": "./scripts/build.sh $npm_package_version",
    "release": "run-s prepare plugin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebastianrothe/wp-datepicker-jquery.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sebastianrothe/wp-datepicker-jquery/issues"
  },
  "homepage": "https://github.com/sebastianrothe/wp-datepicker-jquery#readme",
  "devDependencies": {
    "jest": "^26.6.3",
    "loglevel": "^1.7.1",
    "npm-run-all": "^4.1.5",
    "serve": "^11.3.2"
  },
  "jest": {
    "verbose": true,
    "rootDir": "src",
    "testURL": "https://gruseltour-leipzig.de"
  }
}
