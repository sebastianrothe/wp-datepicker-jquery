{
  "name": "datepicker-jqueryui",
  "version": "1.0.0",
  "description": "A Datepicker in JQuery UI with disabled dates and formatting",
  "main": "src/index.js",
  "scripts": {
    "start": "parcel public/index.html public/data/data.txt",
    "test": "jest --watch",
    "clean": "rm -rf bundle/** && rm -rf build/** && rm -rf dist/**",
    "copy": "./scripts/copy.sh $npm_package_version bundle/custom",
    "build": "parcel build src/**/*.* --out-dir build --experimental-scope-hoisting",
    "bundle": "parcel build public/bundle.html --out-dir bundle --no-content-hash --experimental-scope-hoisting",
    "bundle:custom": "parcel build public/bundle.custom.html --out-dir bundle/custom --no-content-hash --experimental-scope-hoisting",
    "prepare": "run-s clean bundle:custom copy",
    "postversion": "prepare",
    "plugin": "./datepicker-jqueryui-plugin/build.sh $npm_package_version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebastianrothe/wp-datepicker-jquery.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sebastianrothe/wp-datepicker-jquery/issues"
  },
  "homepage": "https://github.com/sebastianrothe/wp-datepicker-jquery#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@getify/eslint-plugin-proper-arrows": "^8.0.1",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-config-prettier-standard": "^2.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^9.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.8.0",
    "loglevel": "^1.6.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.17.0",
    "stylefmt": "^6.0.3",
    "stylelint": "^10.0.1",
    "stylelint-config-standard": "^18.3.0"
  },
  "dependencies": {},
  "jest": {
    "verbose": true,
    "rootDir": "src",
    "testURL": "http://hauntedleipzig.de"
  }
}
